<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<AssemblyName>Havit.NewProjectTemplate.Web.Client</AssemblyName>
		<RootNamespace>Havit.NewProjectTemplate.Web.Client</RootNamespace>
		<BlazorWebAssemblyLoadAllGlobalizationData>true</BlazorWebAssemblyLoadAllGlobalizationData>
	</PropertyGroup>

	<ItemGroup>
	  <Compile Remove="Resources\Model\**" />
	  <Content Remove="Resources\Model\**" />
	  <EmbeddedResource Remove="Resources\Model\**" />
	  <None Remove="Resources\Model\**" />
	</ItemGroup>

	<ItemGroup>
		<PackageReference Include="BlazorApplicationInsights" Version="1.2.1" />
		<PackageReference Include="Blazored.FluentValidation" Version="2.0.1" />
		<PackageReference Include="Blazored.LocalStorage" Version="4.1.2" />
		<PackageReference Include="FluentValidation.DependencyInjectionExtensions" Version="10.3.0" />
		<PackageReference Include="Grpc.Net.Client" Version="2.38.0" />
		<PackageReference Include="Grpc.Net.Client.Web" Version="2.38.0" />
		<PackageReference Include="Havit.Blazor.Components.Web" Version="1.0.20" />
		<PackageReference Include="Havit.Blazor.Components.Web.Bootstrap" Version="1.1.43" />
		<PackageReference Include="Havit.Blazor.Grpc.Client" Version="1.0.6" />
		<PackageReference Include="Havit.Blazor.Grpc.Client.WebAssembly" Version="1.0.6" />
		<PackageReference Include="Havit.Extensions.Localization" Version="1.0.1" />
		<PackageReference Include="Havit.SourceGenerators.StrongApiStringLocalizers" Version="1.0.4" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="5.0.9" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="5.0.9" PrivateAssets="all" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.Authentication" Version="5.0.9" />
		<PackageReference Include="protobuf-net" Version="3.0.101" />
		<PackageReference Include="protobuf-net.Grpc.ClientFactory" Version="1.0.152" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Contracts\Contracts.csproj" />
		<ProjectReference Include="..\Resources\Resources.csproj" />
	</ItemGroup>

</Project>
