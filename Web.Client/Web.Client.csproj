<Project Sdk="Microsoft.NET.Sdk.BlazorWebAssembly">

	<PropertyGroup>
		<TargetFramework>net5.0</TargetFramework>
		<AssemblyName>Havit.NewProjectTemplate.Web.Client</AssemblyName>
		<RootNamespace>Havit.NewProjectTemplate.Web.Client</RootNamespace>
		<BlazorWebAssemblyLoadAllGlobalizationData>true</BlazorWebAssemblyLoadAllGlobalizationData>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="BlazorApplicationInsights" Version="1.1.2" />
		<PackageReference Include="Blazored.LocalStorage" Version="4.1.1" />
		<PackageReference Include="Grpc.Net.Client" Version="2.37.0" />
		<PackageReference Include="Grpc.Net.Client.Web" Version="2.37.0" />
		<PackageReference Include="Havit.Blazor.Components.Web" Version="1.0.13" />
		<PackageReference Include="Havit.Blazor.Components.Web.Bootstrap" Version="1.1.5" />
		<PackageReference Include="Havit.SourceGenerators.StrongApiStringLocalizers" Version="1.0.1" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly" Version="5.0.6" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.DevServer" Version="5.0.6" PrivateAssets="all" />
		<PackageReference Include="Microsoft.AspNetCore.Components.WebAssembly.Authentication" Version="5.0.6" />
		<PackageReference Include="protobuf-net" Version="3.0.101" />
		<PackageReference Include="protobuf-net.Grpc.ClientFactory" Version="1.0.152" />
	</ItemGroup>

	<ItemGroup>
		<ServiceWorker Include="wwwroot\service-worker.js" PublishedContent="wwwroot\service-worker.published.js" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\Contracts\Contracts.csproj" />
	</ItemGroup>

	<ItemGroup>
	  <Folder Include="Resources\Model\" />
	</ItemGroup>

</Project>
